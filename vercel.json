{
    "version": 2,
    "installCommand": "pip install -r requirements.txt",
    "devCommand": "gunicorn app:app",
    "functions": {
        "app.py": {
        "memory": 1024,
        "maxDuration": 60,
        "excludeFiles": ".next/**,*.cache/**,node_modules/**,public/**,app/**,static/**,templates/**,external/**"
        }
    },
    "routes": [
        {
        "src": "/(.*)",
        "dest": "app.py"
        }
    ]
}