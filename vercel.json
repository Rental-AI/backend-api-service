{
    "version": 2,
    "installCommand": "pip install -r requirements.txt",
    "devCommand": "gunicorn app:app",
    "builds": [
        {
        "src": "app.py",
        "use": "@vercel/python"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "app.py"
        }
    ],
    "functions": {
        "/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
    }
}